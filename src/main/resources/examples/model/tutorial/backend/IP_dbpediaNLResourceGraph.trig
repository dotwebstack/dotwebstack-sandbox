GRAPH config:TutorialTheatre {
  config:DbpediaNLResourceGraph a elmo:InformationProduct;
  elmo:appearance elmo:GraphAppearance;
  elmo:endpoint <http://nl.dbpedia.org/sparl>;
  elmo:query '''
    CONSTRUCT {
	  <@SUBJECT@> ?p ?o.
	  <@SUBJECT@> rdfs:label ?slabel.
	  ?o rdfs:label ?olabel
	}
	WHERE {
	  <@SUBJECT@> ?p ?o.
	  <@SUBJECT@> rdfs:label ?slabel.
	  ?o rdfs:label ?olabel
	  FILTER (?p!=<http://dbpedia.org/ontology/wikiPageWikiLink>)
	  }
	'''
  .
}
