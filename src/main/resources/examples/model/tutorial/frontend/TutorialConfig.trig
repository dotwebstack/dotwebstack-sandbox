@prefix elmo:		<http://dotwebstack.org/def/elmo#>.
@prefix config: 	<http://dotwebstack.org/examples/tutorial#>.
@prefix rdfs: 		<http://www.w3.org/2000/01/rdf-schema#>.
@prefix dc:			<http://purl.org/dc/elements/1.1>.
@prefix xsd:		<http://www.w3.org/2001/XMLSchema#>.

# System and configuration graph
GRAPH config: {
	config:TutorialTheatre a elmo:Theatre;
	rdfs:label "The Tutorial theatre, showing users some of the configuration possibilities of dotwebstack";
	dct:title "Tutorial theatre";
	.
	config:TutorialSite a elmo:Site;
		elmo:domain "tutorial.dotwebstack.org";
		.
	config:Mainstage a elmo:Stage;
		elmo:site config:TutorialSite;
		elmo:basePath "main";
	.
	config:MyLocalSparqlBackend a elmo:SparqlBackend;
		elmo:endpoint "http://localhost:8890/sparql"^^xsd:anyURI;
	.
	}
.
