graph config:TestTheatre {

  config:IP_SelectFormatsQuery a elmo:InformationProduct;
    rdfs:label "Cities with populations";
    elmo:query '''
      prefix dbo: <http://dbpedia.org/ontology/>
      select ?s ?s_label (dbo:City as ?type) ?s_count
      where {
        graph <http://localhost:8080/stage> {
          ?s rdf:type dbo:City.
          ?s rdfs:label ?s_label.
          OPTIONAL {?s dbo:population ?s_count}
        }
      }
    ''';
  .

  config:IP_GeoSelectQuery a elmo:InformationProduct;
    rdfs:label "A geometry select";
    elmo:query '''
      prefix geosparql: <http://www.opengis.net/ont/geosparql#>
      select ?p ?p_label ?p_geo
      where {
        graph <http://localhost:8080/stage> {
          ?p geosparql:hasGeometry ?geo.
          ?p rdfs:label ?p_label.
          ?geo geosparql:asWKT ?p_geo.
        }
      }
    ''';
  .
  
  config:IP_GeoConstructQuery a elmo:InformationProduct;
    rdfs:label "A geometry construct";
    elmo:query '''
      prefix geosparql: <http://www.opengis.net/ont/geosparql#>
      construct {
        ?p geosparql:asWKT ?p_geo.
        ?p rdfs:label ?p_label.
      }
      where {
        graph <http://localhost:8080/stage> {
          ?p geosparql:hasGeometry ?geo.
          ?p rdfs:label ?p_label.
          ?geo geosparql:asWKT ?p_geo.
        }
      }
    ''';
  .

  config:IP_SelectFormats_content a elmo:InformationProduct;
    elmo:query '''
        prefix dbo: <http://dbpedia.org/ontology/>
        select ?s ?s_label (dbo:City as ?type) ?s_count
        where {
          ?s rdf:type dbo:City.
          ?s rdfs:label ?s_label.
          OPTIONAL {?s dbo:population ?s_count}
        }
      ''';
  .

  config:IP_ConstructFormats_content a elmo:InformationProduct;
    elmo:query '''
        prefix dbo: <http://dbpedia.org/ontology/>
        prefix testo: <http://localhost:8080/def/test#>
        construct {
          ?s?p?o.
          ?p rdfs:label ?plabel.
          ?p rdfs:comment ?pcomment.
          ?s testo:districtNames ?list.
          ?head rdf:rest ?rest.
          ?head rdf:first ?district.
        }
        where {
          graph <${stage}> {
            ?s rdf:type dbo:City.
            ?s?p?o.
            OPTIONAL {?p rdfs:label ?plabel}
            OPTIONAL {?p rdfs:comment ?pcomment}
            OPTIONAL {
              ?s testo:districtNames ?list.
              ?list rdf:rest* ?head.
              ?head rdf:rest ?rest.
              ?head rdf:first ?district.
            }
          }
        }
      ''';
  .

}