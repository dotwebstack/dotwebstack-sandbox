graph config:TestTheatre {	

  config:IP_VirtualSparql_resource a elmo:InformationProduct ;
     
    elmo:query '''
      construct {
        <http://www.w3.org/2000/01/rdf-schema#> ?p ?o.
      }
      where {
        service <http://localhost:8080/url> {
          graph <http://www.w3.org/2000/01/rdf-schema#> {
            <http://www.w3.org/2000/01/rdf-schema#> ?p ?o.
          }
        }
      }
    '''
  .

  config:IP_VirtualSparql_dataset  a elmo:InformationProduct ;
       
    elmo:query '''
      select *
      where {
        service <http://localhost:8080/url> {
          graph <http://www.w3.org/2000/01/rdf-schema#> {
            ?class a rdfs:Class.
            ?class rdfs:label ?class_label
          }
        }
      }
    '''
  .

  config:IP_VirtualSparql_resource2  a elmo:InformationProduct ;
       
    elmo:query '''
      construct {
        <http://dbpedia.org/resource/Amersfoort> ?p ?o.
      }
      where {
        service <http://localhost:8080/url> {
          <http://dbpedia.org/resource/Amersfoort> ?p ?o.
          FILTER (?p = rdfs:label || ?p = rdfs:comment)
          FILTER (lang(?o)="" or lang(?o)="${language}")
        }
      }
    '''
  .
}