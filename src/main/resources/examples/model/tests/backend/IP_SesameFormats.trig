graph config:TestTheatre {

  config:IP_Sesame_SelectFormats a elmo:InformationProduct;

    elmo:query '''
      prefix dbo: <http://dbpedia.org/ontology/>
      select ?s ?s_label (dbo:City as ?type)
      where {
        ?s rdf:type dbo:City.
        ?s rdfs:label ?s_label.
      }
    ''';
  .

  config:IP_Sesame_SelectFormats_content a elmo:InformationProduct;

    elmo:query '''
      prefix dbo: <http://dbpedia.org/ontology/>
      select ?s ?s_label (dbo:City as ?type)
      where {
        ?s rdf:type dbo:City.
        ?s rdfs:label ?s_label.
      }
    ''';
  .

  config:IP_Sesame_ConstructFormats a elmo:InformationProduct;
	
    elmo:query '''
      prefix dbo: <http://dbpedia.org/ontology/>
      construct {
        ?s?p?o
      }
      where {
        ?s rdf:type dbo:City.
        ?s?p?o.
      }
    ''';
  .

  config:IP_Sesame_ConstructFormats_content a elmo:InformationProduct;
	
    elmo:query '''
      prefix dbo: <http://dbpedia.org/ontology/>
      construct {
        ?s?p?o
      }
      where {
        ?s rdf:type dbo:City.
        ?s?p?o.
      }
    ''';
  .
}