graph stage:TestStage {
  stage:BlankNodes a elmo:Representation;
    elmo:urlPattern "/blank-nodes$";
    elmo:contains stage:HeaderAppearance;
    elmo:contains stage:NavbarSearchAppearance;
    elmo:contains stage:BlankNodes_content;
    elmo:contains stage:BlankNodes_tab;
    elmo:contains stage:BlankNodes_data;
  .

  stage:BlankNodes_data a elmo:Representation;
    elmo:index "0";
  #  elmo:appearance elmo:HiddenAppearance;
    elmo:informationProduct stage:IP_BlankNodes_data
  .

  stage:BlankNodes_tab a elmo:Representation;
    elmo:index "1";
    elmo:appearance elmo:IndexAppearance;
    elmo:data [
      elmo:name "format";
      rdfs:label "xml|json|ttl";
      rdf:value "xml|json|ttl";
    ];
  .

  stage:BlankNodes_content a elmo:Representation;
    elmo:index "2";
    elmo:informationProduct stage:IP_BlankNodes_content
  .
}