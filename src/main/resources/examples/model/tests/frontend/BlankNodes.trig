graph config:TestStage {
  
  config:BlankNodes a elmo:Representation;
    elmo:urlPattern "/blank-nodes$";
    elmo:contains config:HeaderAppearance;
    elmo:contains config:NavbarSearchAppearance;
    elmo:contains config:BlankNodes_content;
    elmo:contains config:BlankNodes_tab;
    elmo:contains config:BlankNodes_data;
  .

  config:BlankNodes_data a elmo:Representation;
    elmo:index "0";
  #  elmo:appearance elmo:HiddenAppearance;
    elmo:informationProduct config:IP_BlankNodes_data
  .

  config:BlankNodes_tab a elmo:Representation;
    elmo:index "1";
    elmo:appearance elmo:IndexAppearance;
    elmo:data [
      elmo:name "format";
      rdfs:label "xml|json|ttl";
      rdf:value "xml|json|ttl";
    ];
  .

  config:BlankNodes_content a elmo:Representation;
    elmo:index "2";
    elmo:informationProduct config:IP_BlankNodes_content
  .
}