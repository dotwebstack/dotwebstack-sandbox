graph config:TestStage {

config:GraphAppearance  a  elmo:Representation;
  elmo:url-pattern  "/graph-appearance$";
  elmo:contains config:HeaderAppearance;
  elmo:contains config:NavbarSearchAppearance;
  elmo:contains config:GraphAppearance_tab;
  elmo:contains config:GraphAppearance_content;
.
config:GraphAppearance_tab elmo:index "1";
  elmo:appearance elmo:IndexAppearance;
  elmo:data [
    html:link "/resource?representation=http%3A%2F%2Flocalhost%3A8080%2Fconfig%23GraphAppearance_content&subject=http://localhost:8080/id/character/Bilbo";
    elmo:name "format";
    rdfs:label "xml|json|ttl|csv|txt|excel|word|pdf|xmi|svgi|d3json|graphml|yed|query|exml|rdfa";
    rdf:value "xml|json|ttl|csv|txt|xlsx|docx|pdf|xmi|svgi|d3json|graphml|yed|query|exml|rdfa";
  ];
.
config:GraphAppearance_content elmo:index "2";
  elmo:appearance elmo:GraphAppearance;
  elmo:fragment [
    elmo:applies-to "Species";
    html:stylesheet '''
      fill: #E0E0E0;
      fill-opacity: .3;
      stroke: #666;
    ''';
  ];
  elmo:fragment [
    elmo:applies-to "Hobbit";
    html:stylesheet '''
      fill: red;
      fill-opacity: .3;
      stroke: #666;
    ''';
  ];
  elmo:fragment [
    elmo:applies-to "Group";
    html:stylesheet '''
      fill: gray;
      fill-opacity: .3;
      stroke: #666;
    ''';
  ];
  elmo:fragment [
    elmo:applies-to "Wizard";
    html:stylesheet '''
      fill: blue;
      fill-opacity: .3;
      stroke: #666;
    ''';
  ];
  elmo:fragment [
    elmo:applies-to "Dwarf";
    html:stylesheet '''
      fill: brown;
      fill-opacity: .3;
      stroke: #666;
    ''';
  ];
  elmo:fragment [
    elmo:applies-to "Elf";
    html:stylesheet '''
      fill: green;
      fill-opacity: .3;
      stroke: #666;
    ''';
  ];
  elmo:fragment [
    elmo:applies-to "Man";
    html:stylesheet '''
      fill: orange;
      fill-opacity: .3;
      stroke: #666;
    ''';
  ];
  elmo:informationProduct config:IP_GraphAppearance_content
  .
}




