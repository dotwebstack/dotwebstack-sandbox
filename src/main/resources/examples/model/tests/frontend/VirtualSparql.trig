
graph stage:TestStage {
  stage:VirtualSparql a elmo:Representation;
    elmo:url-pattern "/virtual-sparql$";
    elmo:index "1";
    elmo:contains stage:HeaderAppearance;
    elmo:contains stage:NavbarSearchAppearance;
    elmo:contains stage:VirtualSparql_resource;
    elmo:contains stage:VirtualSparql_dataset;
    elmo:contains stage:VirtualSparql_resource2;
  .

  # ------------------------------------------------------------------------------------------------- IP_VirtualSparql_resource ---------------------------------------------------------

  stage:VirtualSparql_resource a elmo:Representation;
    elmo:index "1";
    elmo:informationProduct stage:IP_VirtualSparql_resource
  .

  # ------------------------------------------------------------------------------------------------- IP_VirtualSparql_dataset ---------------------------------------------------------

  stage:VirtualSparql_dataset a elmo:Representation;
    elmo:index "2";
    elmo:informationProduct stage:IP_VirtualSparql_dataset
  .

  # ------------------------------------------------------------------------------------------------- IP_VirtualSparql_resource2 ---------------------------------------------------------

  stage:VirtualSparql_resource2 a elmo:Representation;
    elmo:index "3";   
    elmo:informationProduct stage:IP_VirtualSparql_resource2
  .	
}