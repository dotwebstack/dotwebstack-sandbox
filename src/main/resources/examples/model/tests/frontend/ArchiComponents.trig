    elmo:contains stage:NavbarSearchAppearance;
    elmo:contains stage:TableArchiComponents;
  .
  
  stage:TableArchiComponents a elmo:Representation;
    elmo:fragment [
      elmo:appliesTo "type";
      rdfs:label "Type";
