graph config:Testconfig {

  config:SelectFormats a elmo:Representation;
    elmo:url-pattern "/query-selectformats";
    elmo:url-pattern "/api-selectformats";
    elmo:contains config:HeaderAppearance;
    elmo:contains config:NavbarSearchAppearance;
    elmo:contains config:Formats_tab;
    elmo:contains config:SelectFormats_content;
    elmo:contains config:FooterAppearance;
    elmo:index "0";
    elmo:appearance elmo:HiddenAppearance;
    elmo:informationProduct config:IP_SelectFormatsQuery;
  .

  config:Formats_tab elmo:index "1";
    elmo:appearance elmo:IndexAppearance;
    elmo:data [
      html:link "";
      elmo:name "format";
      rdfs:label "xml|json|ttl|csv|txt|excel|word|pdf|xmi|svgi|d3json|graphml|yed|query|exml|rdfa";
      rdf:value "xml|json|ttl|csv|txt|xlsx|docx|pdf|xmi|svgi|d3json|graphml|yed|query|exml|rdfa";
    ];
  .

  config:SelectFormats_content a elmo:Representation;
    elmo:index "2";
    elmo:informationProduct config:IP_SelectFormats_content;
  .
     
  config:ConstructFormats a elmo:Representation;
    elmo:url-pattern "/query-constructformats";
    elmo:url-pattern "/api-constructformats";
    elmo:contains config:HeaderAppearance;
    elmo:contains config:NavbarSearchAppearance;
    elmo:contains config:Formats_tab;
    elmo:contains config:ConstructFormats_content;
    elmo:contains config:FooterAppearance;
  .
  config:Formats_constructTabs elmo:index "1";
    elmo:appearance elmo:IndexAppearance;
    elmo:data [
      html:link "#";
      elmo:name "format";
      rdfs:label "rdf|json|ttl|csv|excel";
      rdf:value "rdf|json|ttl|csv|excel";
    ];
  .
  config:ConstructFormats_content a elmo:Representation;
    elmo:index "2";
    elmo:informationProduct config:IP_ConstructFormats_content;
    
}