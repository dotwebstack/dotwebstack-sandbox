graph config:TestStage {

  # --------------------------------------------------------------------------------------------------------------------------------------  VocabularyAppearance -------------------------------------------------

  config:VocabularyAppearance	a	elmo:Representation;
    elmo:url-pattern	"/lotr(\\.(xml|ttl|json))?$";
    elmo:contains config:HeaderAppearance;
    elmo:contains config:NavbarSearchAppearance;
    elmo:contains config:VocabularyAppearanceQuery
  .
  
  config:VocabularyAppearanceQuery a elmo:Representation;
    elmo:appearance elmo:VocabularyAppearance;
    elmo:informationProduct config:IP_VocabularyQuery
  .

  # --------------------------------------------------------------------------------------------------------------------------------------  UploadedVocabulary -------------------------------------------------

  config:UploadedVocabulary a elmo:Representation;
    elmo:url-pattern "/uploaded-vocabulary$";
    elmo:contains config:HeaderAppearance;
    elmo:contains config:NavbarSearchAppearance;
    elmo:contains config:UploadedVocabularyQuery
  .
  
  config:UploadedVocabularyQuery a elmo:Representation;
    elmo:appearance elmo:VocabularyAppearance;
    elmo:informationProduct config:IP_UploadedVocabulary
  .

  # --------------------------------------------------------------------------------------------------------------------------------------  UploadedModelAppearance -------------------------------------------------

  config:UploadedModelAppearance a elmo:Representation;
    elmo:url-pattern "/uploaded-model";
    elmo:index "1";
    elmo:contains config:HeaderAppearance;
    elmo:contains config:NavbarSearchAppearance;
    elmo:contains config:UploadedModelAppearanceQuery
  .
  
  config:UploadedModelAppearanceQuery a elmo:Representation;
    elmo:appearance elmo:ModelAppearance;
    elmo:informationProduct config:IP_UploadedModelAppearance
  .

  # --------------------------------------------------------------------------------------------------------------------------------------  UploadedShapes -------------------------------------------------

  config:UploadedShapes a elmo:Representation;
    elmo:url-pattern "/uploaded-shapes$";
    elmo:contains config:HeaderAppearance;
    elmo:contains config:NavbarSearchAppearance;
    elmo:contains config:UploadedShapesQuery
  .
  
  config:UploadedShapesQuery a elmo:Representation;
    elmo:appearance elmo:VocabularyAppearance;
    elmo:informationProduct config:IP_UploadedShapesQuery
  .

  # --------------------------------------------------------------------------------------------------------------------------------------  UploadedShapesModel -------------------------------------------------

  config:UploadedShapesModel a elmo:Representation;
    elmo:url-pattern "/uploaded-shapes-model";
    elmo:index "1";
    elmo:contains config:HeaderAppearance;
    elmo:contains config:NavbarSearchAppearance;
    elmo:contains config:UploadedShapesModelQuery
  .
  
  config:UploadedShapesModelQuery a elmo:Representation;
    elmo:appearance elmo:ModelAppearance;	
    elmo:informationProduct config:IP_UploadedShapesQuery
  .

  # --------------------------------------------------------------------------------------------------------------------------------------  UploadedShapesAppearanceYed -------------------------------------------------

  config:UploadedShapesAppearanceYed a elmo:Representation;
    elmo:url-pattern	"/uploaded-shapes\\.yed$";
    elmo:layer elmo:TopLayer;
    elmo:fragment [
      elmo:applies-to rdfs:subClassOf;
      rdfs:label "";
      yed:sourceArrow "none";
      yed:targetArrow "white_delta";
    ];
    elmo:informationProduct config:IP_UploadedShapesAppearanceYed
  .

  # --------------------------------------------------------------------------------------------------------------------------------------  UploadedVocabularyAppearanceYed -------------------------------------------------	

  config:UploadedVocabularyAppearanceYed a elmo:Representation;
    elmo:url-pattern	"/uploaded-vocabulary\\.yed$";
    elmo:layer elmo:TopLayer;
    elmo:fragment [
      elmo:applies-to rdfs:subClassOf;
      rdfs:label "";
      yed:sourceArrow "none";
      yed:targetArrow "white_delta";
    ];
    elmo:informationProduct config:IP_UploadedVocabularyAppearanceYed
  .

  # --------------------------------------------------------------------------------------------------------------------------------------  VocabularyAppearanceYed -------------------------------------------------

  config:VocabularyAppearanceYed a elmo:Representation;
    elmo:url-pattern	"/lotr\\.yed?$";
    elmo:layer elmo:TopLayer;
    elmo:fragment [
      elmo:applies-to rdfs:subClassOf;
      rdfs:label "";
      yed:sourceArrow "none";
      yed:targetArrow "white_delta";
    ];
    elmo:informationProduct config:IP_VocabularyAppearanceYed
  .
}