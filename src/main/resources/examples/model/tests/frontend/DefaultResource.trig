graph config:Testconfig {

  config:DefaultResource a elmo:Representation;
    elmo:uri-pattern "^http://localhost:8080/id/";
    elmo:uri-pattern "^http://localhost:8080/ldt/id/";
    elmo:url-pattern "/resource";
    elmo:index "1";
    elmo:contains config:HeaderAppearance;
    elmo:contains config:NavbarSearchAppearance;
    elmo:contains config:DefaultResource_graph;
    elmo:layer elmo:BottomLayer;
    elmo:informationProduct config:IP_DefaultResource;
    .
    

  config:DefaultResource_graph a elmo:Representation;
    elmo:index "2";
    elmo:appearance elmo:GraphAppearance;
    elmo:fragment [
      elmo:applies-to "rdfs-Class";
      html:stylesheet '''
        fill: blue;
        fill-opacity: .3;
        stroke: #666;
      ''';
    ];
    elmo:fragment [
      elmo:applies-to "owl-Class";
      html:stylesheet '''
        fill: blue;
        fill-opacity: .3;
        stroke: #666;
      ''';
    ];
    elmo:fragment [
      elmo:applies-to "skos-Concept";
      html:stylesheet '''
        fill: red;
        fill-opacity: .3;
        stroke: #666;
      ''';
    ];
    elmo:informationProduct config:IP_DefaultResource_graph;
    .
  
  
}