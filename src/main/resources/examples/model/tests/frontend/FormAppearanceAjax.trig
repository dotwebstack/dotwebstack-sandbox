graph config:Testconfig {

  config:FormAppearanceAjax	a	elmo:Representation;
    elmo:url-pattern	"/query-formappearanceajax$";
    elmo:contains config:HeaderAppearance;
    elmo:contains config:NavbarSearchAppearance;
    elmo:contains	config:FormAppearanceAjax-species;
    elmo:contains	config:FormAppearanceAjax-persons;
    elmo:contains	config:FormAppearanceAjax-cities;
    elmo:queryForm config:FormAppearanceAjax-form;
    elmo:fragment [
      elmo:applies-to "species";
      rdfs:label "Soort"@nl;
      rdfs:label "Species"@en;
    ];
    elmo:fragment [
      elmo:applies-to rdfs:label;
      elmo:appearance elmo:HiddenAppearance;
    ];
    elmo:informationProduct config:IP_FormAppearanceAjax;
    .

  config:FormAppearanceAjax-form a elmo:Form;
    elmo:fragment [
      elmo:applies-to "species";
      rdfs:label "Soort"@nl;
      rdfs:label "Species"@en;
      elmo:constraint elmo:MandatoryConstraint;
      elmo:valuesFrom config:FormAppearanceAjax-species;
      elmo:index "1";
    ];
    elmo:fragment [
      elmo:applies-to "name";
      rdfs:label "Naam"@nl;
      rdfs:label "Name"@en;
      elmo:constraint elmo:MandatoryConstraint;
      elmo:valuesFrom config:FormAppearanceAjax-persons;
      elmo:index "2";
    ];
    elmo:fragment [
      elmo:applies-to "city";
      rdfs:label "Stad (dbpedia)"@nl;
      rdfs:label "City (dbpedia)"@en;
      elmo:constraint elmo:MandatoryConstraint;
      elmo:valuesFrom config:FormAppearanceAjax-cities;
      elmo:index "3";
    ];
    elmo:fragment [
      elmo:appearance elmo:SubmitAppearance;
      rdfs:label "Search"@nl;
      elmo:index "4";
    ];
  .

  config:FormAppearanceAjax-species elmo:appearance elmo:HiddenAppearance;
    elmo:with-parameter "species";
    elmo:informationProduct config:IP_FormAppearanceAjax-species;
    .
    
  config:FormAppearanceAjax-persons elmo:appearance elmo:HiddenAppearance;
    elmo:with-parameter "species";
    elmo:informationProduct config:IP_FormAppearanceAjax-persons;
    .
    
  config:FormAppearanceAjax-cities elmo:appearance elmo:HiddenAppearance;
    elmo:with-parameter "city";
    elmo:with-parameter "name";
    elmo:endpoint <http://dbpedia.org/sparql>;
     elmo:informationProduct config:IP_FormAppearanceAjax-cities;
    .
     
}