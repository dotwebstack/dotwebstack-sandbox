graph config:TestStage {

  config:HtmlAppearance a elmo:Representation;
    elmo:url-pattern "^http(s|)://[^/]+(/(?!resource)[^/]+)?(/|)$";
    elmo:index "1";
    elmo:contains config:HeaderAppearance;
    elmo:contains config:NavbarSearchAppearance;
      elmo:contains config:FooterAppearance;
    elmo:appearance elmo:HtmlAppearance;
    elmo:data [
      rdfs:label "HtmlAppearance (en)"@en;
      rdfs:label "HtmlAppearance (nl)"@nl;
      elmo:html '''
        <p>Test environment & demo (default)</p>
      ''';
    ]
  .
  config:HtmlAppearanceWithoutParameterTypeAndSort a elmo:Representation;
    elmo:url-pattern "/paramtest$";
    elmo:index "1";
    elmo:contains config:HeaderAppearance;
    elmo:contains config:NavbarSearchAppearance;
    elmo:contains config:IndexAppearanceWithoutParameterTypeAndSort;
    elmo:contains config:IndexAppearanceWithParameterTypeAndSort;
    elmo:contains config:IndexAppearanceWithParameterTypeAndWithoutSort;
    elmo:contains config:IndexAppearanceWithParameterSortAndWithoutType;
    elmo:without-parameter "type";
    elmo:without-parameter "sort";
    elmo:appearance elmo:HtmlAppearance;
    elmo:data [
      rdfs:label "HtmlAppearance (en)"@en;
      rdfs:label "HtmlAppearance (nl)"@nl;
      elmo:html '''
        <p>Test environment zonder parameter type en sort</p>
      ''';
    ]
  .
  config:IndexAppearanceWithoutParameterTypeAndSort a elmo:Part;
    elmo:index "0";
    elmo:without-parameter "type";
    elmo:without-parameter "sort";
    elmo:appearance elmo:IndexAppearance;
    elmo:data [
      elmo:name "set";
      rdfs:label "Type|Sort|Type+Sort";
      rdf:value "&type=|&sort=|&type=&sort=";
    ];
  .

  config:HtmlAppearanceWithParameterTypeAndSort a elmo:Representation;
    elmo:url-pattern "/paramtest$";
    elmo:index "1";
    elmo:contains config:HeaderAppearance;
    elmo:contains config:NavbarSearchAppearance;
    elmo:contains config:IndexAppearanceWithoutParameterTypeAndSort;
    elmo:contains config:IndexAppearanceWithParameterTypeAndSort;
    elmo:contains config:IndexAppearanceWithParameterTypeAndWithoutSort;
    elmo:contains config:IndexAppearanceWithParameterSortAndWithoutType;
    elmo:appearance elmo:HtmlAppearance;
    elmo:with-parameter "type";
    elmo:with-parameter "sort";
    elmo:data [
      rdfs:label "HtmlAppearance (en)"@en;
      rdfs:label "HtmlAppearance (nl)"@nl;
      elmo:html '''
        <p>Test environment met parameter type en sort</p>
      ''';
    ]
  .
  config:IndexAppearanceWithParameterTypeAndSort a elmo:Part;
    elmo:index "0";
    elmo:with-parameter "type";
    elmo:with-parameter "sort";
    elmo:appearance elmo:IndexAppearance;
    elmo:data [
      elmo:name "set";
      rdfs:label "NONE|Type|Sort";
      rdf:value "|&type=|&sort=";
    ];
  .

  config:HtmlAppearanceWithParameterTypeAndWithoutSort a elmo:Representation;
    elmo:url-pattern "/paramtest$";
    elmo:index "1";
    elmo:contains config:HeaderAppearance;
    elmo:contains config:NavbarSearchAppearance;
    elmo:contains config:IndexAppearanceWithoutParameterTypeAndSort;
    elmo:contains config:IndexAppearanceWithParameterTypeAndSort;
    elmo:contains config:IndexAppearanceWithParameterTypeAndWithoutSort;
    elmo:contains config:IndexAppearanceWithParameterSortAndWithoutType;
    elmo:appearance elmo:HtmlAppearance;
    elmo:with-parameter "type";
    elmo:without-parameter "sort";
    elmo:data [
      rdfs:label "HtmlAppearance (en)"@en;
      rdfs:label "HtmlAppearance (nl)"@nl;
      elmo:html '''
        <p>Test environment met parameter type en zonder sort</p>
      ''';
    ]
  .
  config:IndexAppearanceWithParameterTypeAndWithoutSort a elmo:Part;
    elmo:index "0";
    elmo:with-parameter "type";
    elmo:without-parameter "sort";
    elmo:appearance elmo:IndexAppearance;
    elmo:data [
      elmo:name "set";
      rdfs:label "NONE|Sort|Type+Sort";
      rdf:value "|&sort=|&type=&sort=";
    ];
  .

  config:HtmlAppearanceWithParameterSortAndWithoutType a elmo:Representation;
    elmo:url-pattern "/paramtest$";
    elmo:index "1";
    elmo:contains config:HeaderAppearance;
    elmo:contains config:NavbarSearchAppearance;
    elmo:contains config:IndexAppearanceWithoutParameterTypeAndSort;
    elmo:contains config:IndexAppearanceWithParameterTypeAndSort;
    elmo:contains config:IndexAppearanceWithParameterTypeAndWithoutSort;
    elmo:contains config:IndexAppearanceWithParameterSortAndWithoutType;
    elmo:appearance elmo:HtmlAppearance;
    elmo:with-parameter "sort";
    elmo:without-parameter "type";
    elmo:data [
      rdfs:label "HtmlAppearance (en)"@en;
      rdfs:label "HtmlAppearance (nl)"@nl;
      elmo:html '''
        <p>Test environment met parameter sort en zonder type</p>
      ''';
    ]
  .
  config:IndexAppearanceWithParameterSortAndWithoutType a elmo:Part;
    elmo:index "0";
    elmo:with-parameter "sort";
    elmo:without-parameter "type";
    elmo:appearance elmo:IndexAppearance;
    elmo:data [
      elmo:name "set";
      rdfs:label "NONE|Type|Type+Sort";
      rdf:value "|&type=|&type=&sort=";
    ];
  .
}