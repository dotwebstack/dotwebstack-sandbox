graph config:TestStage {

  config:Sesame_SelectFormats a elmo:Representation;
    elmo:url-pattern "/sesame-selectformats";
    elmo:contains config:HeaderAppearance;
    elmo:contains config:NavbarSearchAppearance;
    elmo:contains config:Formats_tab;
    elmo:contains config:SelectFormats_content;
    elmo:index "0";
    #elmo:appearance elmo:HiddenAppearance;
    elmo:endpoint <http://localhost:7200/repositories/data>;
    elmo:informationProduct config:IP_Sesame_SelectFormats
.

  config:Sesame_Formats_tab elmo:index "1";
    elmo:appearance elmo:IndexAppearance;
    elmo:data [
      html:link "";
      elmo:name "format";
      rdfs:label "xml|json|ttl|csv|txt|excel|word|pdf|xmi|svgi|d3json|graphml|yed|query|exml|rdfa";
      rdf:value "xml|json|ttl|csv|txt|xlsx|docx|pdf|xmi|svgi|d3json|graphml|yed|query|exml|rdfa";
    ];
.

  config:Sesame_SelectFormats_content a elmo:Representation;
    elmo:index "2";
    elmo:endpoint <http://localhost:7200/repositories/data>;
    elmo:informationProduct config:IP_Sesame_SelectFormats_content
.
  
  config:Sesame_ConstructFormats a elmo:Representation;
    elmo:url-pattern "/sesame-constructformats";
    elmo:contains config:HeaderAppearance;
    elmo:contains config:NavbarSearchAppearance;
    elmo:contains config:Formats_tab;
    elmo:contains config:ConstructFormats_content;
    elmo:index "0";
    #elmo:appearance elmo:HiddenAppearance;
    elmo:endpoint <http://localhost:7200/repositories/data>;  
    elmo:informationProduct config:IP_Sesame_ConstructFormats
.  

  config:Sesame_Formats_constructTabs elmo:index "1";
    elmo:appearance elmo:IndexAppearance;
    elmo:data [
      html:link "#";
      elmo:name "format";
      rdfs:label "rdf|json|ttl|csv|excel";
      rdf:value "rdf|json|ttl|csv|excel";
    ];
.

  config:Sesame_ConstructFormats_content a elmo:Representation;
    elmo:index "2";
    elmo:endpoint <http://localhost:7200/repositories/data>;
    elmo:informationProduct config:IP_Sesame_ConstructFormats_content
.  
}