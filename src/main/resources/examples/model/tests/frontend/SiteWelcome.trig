graph config:TestStage {

  config:HtmlAppearance a elmo:Representation;
    elmo:url-pattern "/stagename-substagename-welcome$";
    elmo:index "1";
    elmo:appearance elmo:HtmlAppearance;
    elmo:data [
      rdfs:label "Stage test (en)"@en;
      elmo:html '''
        <p>This is a test for a specific stage. The stagename containts a slash, and is available at "/stagename/substagename".</p>
      ''';
    ]
  .
}